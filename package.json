{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:clean": "cd android && ./gradlew clean",
    "android": "SET ENVFILE=.env.development && react-native run-android",
    "android:staging": "SET ENVFILE=.env.staging && react-native run-android",
    "android:prod": "SET ENVFILE=.env.production && react-native run-android",
    "build": "cd android && SET ENVFILE=.env.development ./gradlew assembleRelease",
    "build:staging": "cd android && SET ENVFILE=.env.staging ./gradlew assembleRelease",
    "build:prod": "cd android && SET ENVFILE=.env.production ./gradlew assembleRelease",
    "build:bundle": "cd android && SET ENVFILE=.env.development ./gradlew bundleRelease",
    "build:bundle:staging": "cd android && SET ENVFILE=.env.staging ./gradlew bundleRelease",
    "build:bundle:prod": "cd android && SET ENVFILE=.env.production ./gradlew bundleRelease",
    "ios": "ENVFILE=.env.development react-native run-ios",
    "ios:staging": "ENVFILE=.env.staging react-native run-ios",
    "ios:prod": "ENVFILE=.env.production react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/elements": "^2.2.6",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/native-stack": "^7.2.1",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-i18next": "^15.4.1",
    "react-native": "0.78.0",
    "react-native-config": "^1.5.5",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.0",
    "@react-native/eslint-config": "0.78.0",
    "@react-native/metro-config": "0.78.0",
    "@react-native/typescript-config": "0.78.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-i18next": "^8.1.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
